{
    "semi": true,
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 4,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "ignore",
    "endOfLine": "lf",
    "overrides": [
        {
            "files": "*.html",
            "options": {
                "parser": "angular",
                "printWidth": 150
            }
        }
    ]
}
